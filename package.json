{
  "name": "thekabar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:native": "vite build --base=./ --mode native",
    "icons": "node scripts/generate-icons.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:dev": "nodemon --watch src --watch server --ext js,mjs,cjs,jsx,ts,tsx server/index.js",
    "dev:all": "concurrently \"npm:server:dev\" \"npm:dev\"",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap add android",
    "cap:open:android": "npx cap open android",
    "android": "npm run build:native && npx cap sync android && npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.24",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.3.2",
    "vite": "^7.1.2"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
